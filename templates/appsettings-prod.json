{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "app_service_name": {
            "type": "string"
        },
        "app_settings": {
            "type": "array"
        }
    },
    "functions": [],
    "variables": {
        "local_app_settings": [
            { 
                "name": "Custom1Setting1",
                "value": "value1"
            },
            { 
                "name": "Custom1Setting2",
                "value": "value2"
            }
        ]
    },
    "resources": [
        {
            "name": "[concat(parameters('app_service_name'), '/appsettings')]",
            "type": "Microsoft.Web/sites/config",
            "apiVersion": "2018-02-01",
            "properties": "[union(variables('local_app_settings'))]"
        }
    ],
    "outputs": {}
}